<h1>Providers</h1>
<br>

<table>
    <thead 
    >
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => {
            const {first_name, last_name, provider_specialty} = row
        %>
            <tr>
                <td><%= first_name %></td>
                <td><%= last_name %></td>
                <td><%= provider_specialty %></td>
            </tr>
        <% }) %>
        
    </tbody>
</table>