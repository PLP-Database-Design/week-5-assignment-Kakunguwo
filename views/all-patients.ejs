<h1>Patients</h1>
<br>

<table>
    <thead 
    >
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date of Birth</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row => {
            const {patient_id, first_name, last_name, date_of_birth} = row
        %>
            <tr>
                <td><%= patient_id %></td>
                <td><%= first_name %></td>
                <td><%= last_name %></td>
                <td><%= date_of_birth %></td>
            </tr>
        <% }) %>
        
    </tbody>
</table>